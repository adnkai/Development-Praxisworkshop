{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingExcludedTypes": "Request",
            "samplingSettings": {
                "isEnabled": true
            }
        }
    },
    "extensions": {
        "queues": {
            "maxPollingInterval": "00:01:00",
            "visibilityTimeout" : "00:00:30",
            "batchSize": 1,
            "maxDequeueCount": 3,
            "newBatchThreshold": 1,
            "messageEncoding": "none"
        }
    }
}